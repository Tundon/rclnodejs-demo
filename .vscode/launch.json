  "version": "0.2.0",
  "configurations": [
    {
      "name": "ros.ts",
      "request": "launch",
      "type": "node",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/ros.ts",
      "runtimeExecutable": "ts-node",
      "args": [
        "monitor", "/a", "/b", "--", "/c"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Script",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${file}",
      "runtimeExecutable": "ts-node",
      // "outFiles": [
      //   "${workspaceFolder}/**/*.js"
      // ]
    }
  ]
}